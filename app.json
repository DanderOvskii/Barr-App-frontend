{
  "expo": {
    "name": "Barr-App",
    "slug": "Barr-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/barapplogodeel2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/barapplogodeel2.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.danderorthofoodie.BarrApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/barapplogodeel2.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/barapplogodeel2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "fonts": [
      {
        "asset": "./assets/fonts/Roboto-Bold.ttf",
        "family": "Roboto-bold"
      },
      {
        "asset": "./assets/fonts/Roboto-Regular.ttf",
        "family": "Roboto"
      },
      {
        "asset": "./assets/fonts/Resolve-BlackWd.otf",
        "family": "Resolve-BlackWd"
      },
      {
        "asset": "./assets/fonts/AtomicMarker.otf",
        "family": "AtomicMarker"
      }
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
